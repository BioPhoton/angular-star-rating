    "@ngrx/router-store": "12.4.0",
    "@ngrx/store": "12.4.0",
    "@ngrx/store-devtools": "12.4.0",
    "@nrwl/angular": "^12.9.0",
    "@nrwl/nx": "0.9.1",
    "angular-star-rating": "^4.0.0-beta.0",
    "bootstrap": "^4.0.0",
    "core-js": "3.8.3",
    "css-star-rating": "^1.2.4",
    "fontawesome": "^4.7.2",
    "ngrx-store-freeze": "^0.2.1",
    "rxjs": "^6.6.7",
    "rxjs-compat": "^6.0.0-rc.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.5",
    "@angular/cli": "12.2.5",
