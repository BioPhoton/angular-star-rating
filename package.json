    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.13.0",
    "node-sass-tilde-importer": "^1.0.0",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
