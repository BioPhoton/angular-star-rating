    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "rxjs": "^5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0",
    "zone.js": "^0.8.4"
