    "@nrwl/tao": "^12.9.0",
    "@nrwl/workspace": "^12.9.0",
    "@types/jasmine": "3.6.3",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "14.14.33",
    "codelyzer": "6.0.2",
    "compare-func": "^1.3.2",
    "copyfiles": "2.4.1",
    "cosmiconfig": "^7.0.1",
    "cypress": "^8.3.1",
    "github-url-from-git": "^1.5.0",
    "jasmine": "^3.8.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
